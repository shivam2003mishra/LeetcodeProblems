package Leetcode;

public class reverseIntegerNumber {
    public static int reverse(int x) {
        int res=0;
        while(x!=0){
            int temp=x%10;
            res = res*10+temp;
            x =x/10;
        }
        return res;
    }
    public static void main(String[] args) {
        System.out.println(reverse(-123));
    }
}
